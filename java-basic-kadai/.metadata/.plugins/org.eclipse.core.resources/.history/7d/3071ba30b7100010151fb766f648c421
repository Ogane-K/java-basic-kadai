package kadai_021;

public class DictionaryExec_Chapter21 {

	public static void main(String[] args) {

		//		調べる単語をセット
		String[] searchWord = { "apple", "banana", "grape", "orange" };

		Dictionary_Chapter21 dic = new Dictionary_Chapter21();

		
		
		//		 検索単語を全て検索し、結果を表示する。
		for (int i = 0; searchWord.length < i; i++) {

			dic.chekDictionary(searchWord[i]);

		}

	}

}
